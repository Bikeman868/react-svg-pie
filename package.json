  "peerDependencies": {
    "@material-ui/core": "4 || 5",
    "react": "15 || 16 || 17",
    "react-dom": "15 || 16 || 17"
  },
  "devDependencies": {
    "@babel/cli": "^7.8",
    "@babel/core": "^7.8",
    "@babel/plugin-syntax-export-default-from": "^7.12.13",
    "@babel/preset-env": "^7.8",
